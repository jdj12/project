<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="org.project.mapper.profileMapper">
  
  	<insert id="insert">
  		insert into profile(id,pw,name,phone,address)
		value(#{id},#{pw},#{name},#{phone},#{address})
  	</insert>
 
  	
  	<select id="rogin" resultType="org.project.domain.profileVO">
		select * from profile where id=#{id} and pw=#{pw}  	
  	</select>
  </mapper>